.tabbar-container{
  width: 100%;
  height: 100rpx;
  background-color: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 9999;
  display: flex;
  align-items: flex-end;
  border-top: 1rpx solid #e6e6e6;
  box-sizing: border-box;
}
.tabbar-container .tabbar-select{
  color: #298acb;
}
.tabbar-item{
  flex: 1;
  height: 100rpx;
  padding: 10rpx 0rpx;
  background-color: #fff;
  border-top: 1rpx solid #e6e6e6;
  box-sizing: border-box;
}
.tabbar-item .tabbar-image{
  width: 100%;
  height: 50rpx;
  display: flex;
  justify-content: center;
  position: relative;
}
.tabbar-item .tabbar-image .image{
  width: 50rpx;
  height: 50rpx;
}
.tabbar-item .tabbar-text{
  width: 100%;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 26rpx;
  text-align: center;
}
 
.tabbar-center{
  height: 150rpx;
  border: 0;
  padding: 0;
  background-color: transparent;
  position: relative;
  box-sizing: border-box;
}
.tabbar-center .mask-item{
  width: 100%;
  height: 50rpx;
  overflow: hidden;
  position: relative;
  box-sizing: border-box;
}
.tabbar-center .mask-item .mask-border{
  width: 112rpx;
  height: 112rpx;
  border: 1rpx solid #e6e6e6;
  border-radius: 50%;
  background-color: #fff;
  position: absolute;
  top: 0;
  left: calc(50% - 56rpx);
  box-sizing: border-box;
}
.tabbar-center .item-center{
  width: 100%;
  height: 100rpx;
}
.tabbar-center .item-center .tabbar-image{
  position: absolute;
  display: inline-block;
  background: linear-gradient(to top, #976FFF, #7F4EFD);
  border: solid 4px white;
  width: 96rpx;
  height: 96rpx;
  line-height: 96rpx;
  border-radius: 50%;
  bottom: 50rpx;
  left: 50%;
  margin-left: -48rpx;
  box-shadow: 0 -4px 3px 3px rgb(165 189 251 / 20%);
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-items: center;
}
.tabbar-center .item-center .tabbar-text{
  position: absolute;
  bottom: 10rpx;
  left: 0rpx;
  width: 100%;
}
.tabbar-center .item-center .tabbar-image image{
  width: 36rpx;
  height: 36rpx;
}